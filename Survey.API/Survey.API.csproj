    </Reference>
    <Reference Include="Genesyslab.Platform.Commons.Connection">
      <HintPath>GenesysDLLs\Genesyslab.Platform.Commons.Connection.dll</HintPath>
    </Reference>
    <Reference Include="Genesyslab.Platform.Commons.Protocols">
