    </Reference>
    <Reference Include="Genesyslab.Platform.Configuration.Protocols">
      <HintPath>GenesysDLLs\Genesyslab.Platform.Configuration.Protocols.dll</HintPath>
    </Reference>
    <Reference Include="Genesyslab.Platform.Outbound.Protocols">
      <HintPath>GenesysDLLs\Genesyslab.Platform.Outbound.Protocols.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="GenesysDLLs\Genesyslab.Core.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.ApplicationBlocks.Commons.Broker.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.ApplicationBlocks.Commons.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.ApplicationBlocks.Commons.Protocols.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.ApplicationBlocks.ConfigurationObjectModel.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.ApplicationBlocks.SipEndpoint.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="GenesysDLLs\Genesyslab.Platform.Commons.Collections.dll">
